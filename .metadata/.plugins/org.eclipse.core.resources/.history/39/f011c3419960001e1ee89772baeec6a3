public class GuardarClientePrueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		SessionFactory miFactory = (TransactionFactory) new Configuration().configure("hibernate.cfg.xml").addAnnotatedClass(clientes.class).buildSessionFactory();
		Session miSession = miFactory.
	}

}
